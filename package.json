{
  "name": "KeendooNext",
  "productName": "KeendooNext",
  "description": "Keendoo next events app",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolaschaillot/electron-events.git"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "electron": "^1.4.5",
    "electron-installer-dmg": "^0.1.2",
    "electron-packager": "^8.1.0",
    "electron-winstaller": "^2.5.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "faucet": "0.0.1",
    "rimraf": "^2.5.4",
    "tape": "^4.6.3"
  },
  "scripts": {
    "start": "electron index.js",
    "dev": "NODE_ENV=development npm start",
    "clear-data": "node ./scripts/cleardb.js && node ./scripts/clearauth.js && node ./scripts/clearsynctokens.js",
    "tape": "tape -r babel-register tests/**/*.js | faucet",
    "test": "npm run lint  && npm run tape",
    "lint": "eslint client server tests",
    "build": "electron-packager  --all . MyWFDashboard --out release-builds/ --overwrite",
    "package-mac": "electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=src/client/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/client/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Last events app\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=src/client/assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/KeendooNext-darwin-x64/KeendooNext.app KeendooNextInstaller --out=release-builds --overwrite --icon=src/client/assets/icons/mac/icon.icns",
    "create-installer-win": "node installers/windows/createinstaller.js"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "classnames": "^2.2.5",
    "configstore": "^2.1.0",
    "googleapis": "^59.0.0",
    "leveldown": "^1.5.3",
    "levelup": "^1.3.3",
    "linvodb3": "^3.25.0",
    "lodash": "^4.17.3",
    "md5": "^2.2.1",
    "medeadown": "^1.1.8",
    "moment": "^2.17.1",
    "node-fetch": "^1.6.3",
    "q": "^1.4.1",
    "querystring": "^0.2.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "license": "ISC"
}
